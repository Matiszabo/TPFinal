@using Prac.Models

@{
    ViewData["Title"] = "Home Page";
}
<h1>
@foreach (Viajes item in ViewBag.ListaViajes)
{
    <div>
        <p>@item.nombre</p>
        <p>@item.precio</p>
        <p>@item.imagen</p>
        <p>@item.descripcion</p>
        <p>@item.puntaje</p>
    </div>
}
 <a class="btn btn-info" href='@Url.Action("PaginaPrincipal","Home")'>Volver a Paises</a>

